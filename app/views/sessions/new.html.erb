<h1>Log In!</h1>

<%= form_tag sessions_path do %>
  <%= label_tag :username, 'Enter Username' %>
  <%= text_field_tag :username, params[:username], class: 'form-control' %><br>
  <%= label_tag :password, 'Enter Password' %>
  <%= password_field_tag :password, params[:password], class: 'form-control' %><br>
  <%= submit_tag 'Log In', class: 'btn btn-primary' %>
<% end %>